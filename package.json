{
  "name": "@mctech/antlr4-tool",
  "version": "1.1.4",
  "description": "An Antlr4 tool that generates both Typescript and JavaScript Antlr Parsers (Current Antlr version: 4.8)",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "mctech-antlr4": "src/app.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "node --experimental-import-meta-resolve --experimental-vm-modules ./src/cli.js -l ts -o ./src/test ./samples/c/C.g4"
  },
  "files": [
    "src",
    "LICENSE",
    "docs"
  ],
  "keywords": [
    "ANTLR4",
    "parser",
    "antlr4",
    "es5",
    "typescript"
  ],
  "license": "ISC",
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@swc/core": "^1.2.165",
    "antlr4": "^4.10.1",
    "chalk": "^2.4.1",
    "chdir": "0.0.0",
    "commander": "^2.16.0",
    "ejs": "^2.6.1",
    "find-package-json": "^1.1.0",
    "lodash": "^4.17.10",
    "rollup": "^2.70.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-swc": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcchatman8009/antlr4-tool"
  },
  "author": {
    "name": "Marlon Chatman",
    "email": "mcchatman8009@gmail.com"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
