{
  "name": "@mctech/antlr4-tool",
  "version": "1.1.1",
  "description": "An Antlr4 tool that generates both Typescript and JavaScript Antlr Parsers (Current Antlr version: 4.8)",
  "main": "src/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "mctech-antlr4-tool": "src/cli.js",
    "mctech-antlr4.js": "src/cli.js"
  },
  "scripts": {
    "test": "bash -c 'node --experimental-import-meta-resolve --experimental-vm-modules ./src/cli.js -l ts -o ./src/test ./samples/c/C.g4'",
    "build": "rollup -c",
    "buildDev": "rollup -c --watch --environment=dev"
  },
  "files": [
    "dist",
    "LICENSE",
    "docs"
  ],
  "keywords": [
    "ANTLR4",
    "parser",
    "antlr4",
    "es5",
    "typescript"
  ],
  "license": "ISC",
  "dependencies": {
    "antlr4": "^4.10.0",
    "chalk": "^2.4.1",
    "chdir": "0.0.0",
    "commander": "^2.16.0",
    "ejs": "^2.6.1",
    "find-package-json": "^1.1.0",
    "lodash": "^4.17.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcchatman8009/antlr4-tool"
  },
  "author": {
    "name": "Marlon Chatman",
    "email": "mcchatman8009@gmail.com"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
