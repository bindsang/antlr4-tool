import antlr4 from 'antlr4'
import CommonTokenStream = antlr4.CommonTokenStream
import ParserRuleContext = antlr4.ParserRuleContext
import Token = antlr4.Token
import ErrorNode = antlr4.tree.ErrorNode
import ParseTreeListener = antlr4.tree.ParseTreeListener
import TerminalNode = antlr4.tree.TerminalNode
<%_ _.each(imports, (i) => { _%>
import <%- i.import %> from '<%= i.from %>'
<%_ }); _%>

export default class <%= className %> implements ParseTreeListener {
  constructor();

  visitTerminal (node: TerminalNode): void

  visitErrorNode (node: ErrorNode): void

  enterEveryRule (node: ParserRuleContext): void

  exitEveryRule(node: ParserRuleContext): void
<% _.each(methods, (method) => { %>
  <%= method.enter %> (ctx: <%= method.argType %>): <%= method.returnType %>

  <%= method.exit %> (ctx: <%= method.argType %>): <%= method.returnType %>
<%_ }) _%>
}
